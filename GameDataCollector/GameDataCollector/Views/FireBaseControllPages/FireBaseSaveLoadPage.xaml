<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GameDataCollector.Views.FireBaseControllPages.FireBaseSaveLoadPage">
    <ContentPage.Content>
        <StackLayout Padding="10"
                     x:Name="saveLoadArea">
            <Button Text="Speichern" x:Name="save"/>
            <Entry x:Name="lastSave"/>
            <Button Text="Laden" x:Name="load"/>
            <Entry x:Name="lastLoad"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>